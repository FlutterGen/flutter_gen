{
  "name": "flutter_gen",
  "private": true,
  "author": "wasabeef",
  "scripts": {
    "format": "prettier --config prettier.config.js --write '**/*.+(md|yml|yaml|json)'",
    "prepare": "husky install"
  },
  "lint-staged": {
    ".github/**/*.{yaml,yml}": [
      "node-actionlint"
    ],
    "*.dart": [
      "melos format"
    ]
  },
  "devDependencies": {
    "husky": "8.0.3",
    "lint-staged": "^13.1.0",
    "node-actionlint": "^1.2.2",
    "prettier": "^2.8.3",
    "prettier-plugin-packagejson": "^2.4.0"
  },
  "packageManager": "pnpm@7.26.3"
}
